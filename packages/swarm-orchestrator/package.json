{
  "name": "swarm-orchestrator",
  "version": "0.1.0",
  "description": "Multi-agent orchestration for parallel issue execution with Claude Code",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "swarm-orchestrator": "dist/cli.js",
    "swarm": "dist/cli.js"
  },
  "files": [
    "dist",
    "skills",
    ".claude-plugin",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/cli.ts src/index.ts --format esm --dts",
    "dev": "tsup src/cli.ts src/index.ts --format esm --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "swarm",
    "orchestrator",
    "multi-agent",
    "claude-code",
    "linear",
    "parallel-execution",
    "git-worktree"
  ],
  "author": "sbraind9 <sebastianbrain@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sbraind/mcp-lite-wrappers.git",
    "directory": "packages/swarm-orchestrator"
  },
  "homepage": "https://github.com/sbraind/mcp-lite-wrappers/tree/main/packages/swarm-orchestrator#readme",
  "bugs": {
    "url": "https://github.com/sbraind/mcp-lite-wrappers/issues"
  },
  "dependencies": {
    "commander": "^12.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18"
  }
}
